body
  margin 0
  padding 0

h1, h2, h3, h4, h5, h6
  text-align center
  font-weight normal
  text-rendering optimizelegibility

h1
  color #4E443C
  font-size 36px
  font-weight bold

pre
  border 1px solid #000
  overflow-x auto
  background #222
  color #fff
  text-shadow none

.wrapper
  max-width 750px
  padding 0 20px 40px 20px
  margin auto